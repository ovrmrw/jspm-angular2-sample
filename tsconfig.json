{
  "compilerOptions": {
    "target": "es2015",
    "noImplicitAny": false,
    "sourceMap": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": ".tscOutDir"
  },
  "exclude": [
    "node_modules",
    "jspm_packages"
  ]
}